<p id="notice"><%= notice %></p>

<h1>Sectors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Trainer</th>
	  <th>Value</th>
	  <th>User_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sectors.each do |sector| %>
      <tr>
        <td><%= sector.product_name %></td>
        <td><%= sector.product_count %></td>
		<td><%= sector.value %></td>
		<td><%= sector.user_id %></td> 
        <td><%= link_to 'Show', sector %></td>
        <td><%= link_to 'Edit', edit_sector_path(sector) %></td>
        <td><%= link_to 'Destroy', sector, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sector', new_sector_path %>

<br>

